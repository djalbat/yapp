{
  "name": "yapp",
  "author": "James Smith",
  "version": "0.0.16",
  "license": "MIT, Anti-996",
  "homepage": "https://github.com/djalbat/yapp",
  "description": "Yet Another Pretty Printer.",
  "repository": {
    "type": "git",
    "url": "https://github.com/djalbat/yapp"
  },
  "dependencies": {
    "easy": "^11.1.8",
    "easy-richtextarea": "^3.0.4",
    "easy-with-style": "^3.0.3",
    "necessary": "^4.1.10",
    "occam-dom": "^2.0.20",
    "occam-grammar-utilities": "^4.0.20",
    "occam-lexers": "^11.0.47",
    "occam-parsers": "^11.0.35"
  },
  "devDependencies": {
    "@babel/core": "^7.8.6",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-transform-react-jsx": "^7.8.3",
    "@babel/preset-env": "^7.8.6",
    "browserify": "^16.5.0",
    "concurrently": "^5.2.0",
    "easy-layout": "^5.0.5",
    "express": "^4.17.1",
    "juxtapose": "^3.0.3",
    "lively-cli": "^1.1.17",
    "watchful-cli": "^1.4.6"
  },
  "scripts": {
    "start": "node ./bin/main.js",
    "clean": "rm -rf ./lib",
    "lively": "lively --port=3333 --watch-pattern=./example.js --allowed-origin=http://localhost:8888",
    "watchful": "watchful --source-directory=./es6 --temp-directory=./lib --entry-file=example.js --bundle-file=./example.js --pause=100",
    "batch": "npm run watchful batch --",
    "batch-debug": "npm run watchful batch -- --debug",
    "incremental": "npm run watchful incremental --",
    "incremental-debug": "npm run watchful incremental -- --debug",
    "build": "npm run clean && npm run batch",
    "build-debug": "npm run clean && npm run batch-debug",
    "watch": "npm run clean && npm run batch && concurrently -k 'npm run incremental' 'npm run lively'",
    "watch-debug": "npm run clean && npm run batch-debug && concurrently -k 'npm run incremental-debug' 'npm run lively'"
  },
  "main": "./lib/index.js"
}
