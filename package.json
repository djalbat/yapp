{
  "name": "yapp",
  "author": "James Smith",
  "version": "0.0.8",
  "license": "MIT, Anti-996",
  "homepage": "https://github.com/djalbat/yapp",
  "description": "Yet Another Pretty Printer.",
  "repository": {
    "type": "git",
    "url": "https://github.com/djalbat/yapp"
  },
  "dependencies": {
    "easy": "^10.3.12",
    "easy-richtextarea": "^2.2.33",
    "easy-with-style": "^2.0.24",
    "juxtapose": "^2.0.14",
    "necessary": "^4.1.10",
    "occam-dom": "^2.0.15",
    "occam-grammar-utilities": "^4.0.17",
    "occam-lexers": "^11.0.31",
    "occam-parsers": "^11.0.27"
  },
  "devDependencies": {
    "@babel/core": "^7.8.6",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-transform-react-jsx": "^7.8.3",
    "@babel/preset-env": "^7.8.6",
    "browserify": "^16.5.0",
    "easy-layout": "^4.1.26",
    "watchful-cli": "^1.4.6"
  },
  "scripts": {
    "clean": "rm -rf ./lib",
    "watchful": "watchful --source-directory=./es6 --temp-directory=./lib --entry-file=example.js --bundle-file=./example.js --pause=100",
    "batch": "npm run watchful batch --",
    "batch-debug": "npm run watchful batch -- --debug",
    "incremental": "npm run watchful incremental --",
    "incremental-debug": "npm run watchful incremental -- --debug",
    "build": "npm run clean && npm run batch",
    "build-debug": "npm run clean && npm run batch-debug",
    "watch": "npm run clean && npm run batch && npm run incremental",
    "watch-debug": "npm run clean && npm run batch-debug && npm run incremental-debug"
  },
  "main": "./lib/index.js"
}
